{
    "name": "Bilibili Helper",
    "version": "0.0.0.2",
    "description": "An extension with contents filter and CB history on Bilibili.",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.bilibili.com/*"
            ],
            "js": [
                "mark.js"
            ]
        }
    ],
    "icons": {
        "48": "icons/icon.png"
    }
}